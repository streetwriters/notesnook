  "devDependencies": {
    "@notesnook/core": "file:../core",
    "@types/react": "^18.2.39",
    "react": "18.2.0",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "timeago.js": "4.0.2"
  },
  "dependencies": {
    "@notesnook/core": "file:../core",
    "@readme/data-urls": "^3.0.0",
